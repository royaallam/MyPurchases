<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/my_nav"
    app:startDestination="@id/loginFragment">
    <fragment
        android:id="@+id/loginFragment"
        android:name="com.tuwiaq.mypurchases.LoginFragment.LoginFragment"
        android:label="LoginFragment" >
        <action
            android:id="@+id/action_loginFragment_to_mapSuperMarketFragment2"
            app:destination="@id/mapSuperMarketFragment" />
        <action
            android:id="@+id/action_loginFragment_to_registerFragment3"
            app:destination="@id/registerFragment" />
    </fragment>
    <fragment
        android:id="@+id/registerFragment"
        android:name="com.tuwiaq.mypurchases.RegisterFragment.RegisterFragment"
        android:label="fragment_register"
        tools:layout="@layout/fragment_register" >
        <action
            android:id="@+id/action_registerFragment_to_loginFragment3"
            app:destination="@id/loginFragment" />
    </fragment>
    <fragment
        android:id="@+id/mapSuperMarketFragment"
        android:name="com.tuwiaq.mypurchases.Location.MapSuperMarketFragment"
        android:label="MapSuperMarketFragment" >
        <action
            android:id="@+id/action_mapSuperMarketFragment_to_scanerBarFragment4"
            app:destination="@id/scanerBarFragment4" />
    </fragment>
    <fragment
        android:id="@+id/scanerBarFragment4"
        android:name="com.tuwiaq.mypurchases.Barcode.ScanerBarFragment"
        android:label="fragment_scaner_bar"
        tools:layout="@layout/fragment_scaner_bar" >
        <action
            android:id="@+id/action_scanerBarFragment4_to_listAddSuperFragment"
            app:destination="@id/listAddSuperFragment" />
    </fragment>
    <fragment
        android:id="@+id/listAddSuperFragment"
        android:name="com.tuwiaq.mypurchases.Listitem.ListAddSuperFragment"
        android:label="fragment_list_add_super"
        tools:layout="@layout/fragment_list_add_super" />
</navigation>